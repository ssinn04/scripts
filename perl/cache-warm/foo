foreach $var (@list) {
  if ($verbose) { print "\nLINE: $var\n" };
  @to_split = split(/\s+/, $var);
  $to_grab = $to_split[0];
  if ($verbose) { print "\nto_grab: $to_grab\n"};

  $to_get=url($to_grab);
  if ($verbose) { print "\nto_get:  $to_get\n"};
  my $browser = LWP::UserAgent->new();

  my $response = $browser->get(
    '$to_get')
    or die "Couldn't get $to_get: $!";
